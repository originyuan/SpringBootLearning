<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cc.yuanspace.springboot.integration.mybatis.mapper.UserMapper">

    <select id="selectOne" resultType="cc.yuanspace.springboot.integration.mybatis.entity.UserDO">
    select
        *
    from
        t_user
    where
        user_id = #{userId}
  </select>

</mapper>